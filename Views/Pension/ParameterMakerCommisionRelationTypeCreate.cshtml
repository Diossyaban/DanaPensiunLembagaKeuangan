@model DPLK.Models.CommRltnType

@{
    ViewData["Title"] = "DPLK | Perta Life";
    Layout = "~/Views/Shared/DPLK_Layout.cshtml";
}

<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Pension /</span> Create New Commision Relation Type</h4>

    <form asp-action="ParameterMakerCommisionRelationTypeCreate" method="post">
        <div class="form-group">
            <label for="CommRltnTypeNmbr">ID:</label>
            <input type="text" class="form-control" id="CommRltnTypeNmbr" name="CommRltnTypeNmbr" required>
        </div>
        <div class="form-group">
            <label for="CommRltnTypeNm">Description:</label>
            <input type="text" class="form-control" id="CommRltnTypeNm" name="CommRltnTypeNm" required>
        </div>
        <div class="form-group">
            <label for="OldTypeNmbr">Old Type Number:</label>
            <input type="text" class="form-control" id="OldTypeNmbr" name="OldTypeNmbr">
        </div>
        <div class="form-group">
            <label for="LastChangeDt">Last Change Date:</label>
            <input type="datetime-local" class="form-control" id="LastChangeDt" name="LastChangeDt">
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
        <a asp-action="ParameterMakerCommisionRelationType" class="btn btn-secondary">Back to List</a>
    </form>
</div>
