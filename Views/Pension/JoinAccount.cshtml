
@{
    ViewData["Title"] = "DPLK | Perta Life";
    Layout = "~/Views/Shared/DPLK_Layout.cshtml";
}


<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

<h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light"> Pension / </span> Transaction - Join Account </h4>
<div class="row">
    <div class="container-xxl flex-grow-1 container-p-y">
        <div class="nav-align-top mb-4">
            <ul class="nav nav-pills mb-3" role="tablist">
                <li class="nav-item">
                    <button type="button"
                            class="nav-link active"
                            role="tab"
                            data-bs-toggle="tab"
                            data-bs-target="#navs-pills-top-home"
                            aria-controls="navs-pills-top-home"
                            aria-selected="true">
                        Join Account Process
                    </button>
                </li>
                <li class="nav-item">
                    <button type="button"
                            class="nav-link"
                            role="tab"
                            data-bs-toggle="tab"
                            data-bs-target="#navs-pills-top-profile"
                            aria-controls="navs-pills-top-profile"
                            aria-selected="false">
                        Listing
                    </button>
                </li>
             
            </ul>
            <div class="tab-content">
                <div class="tab-pane fade show active" id="navs-pills-top-home" role="tabpanel">
                    Kosong
                </div>
                <div class="tab-pane fade" id="navs-pills-top-profile" role="tabpanel">
                    @await Html.PartialAsync("AccountListing")

                </div>
             
            

            </div>
        </div>
    </div>

</div>



