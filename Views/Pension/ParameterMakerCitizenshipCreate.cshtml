@model DPLK.Models.Citizenship

@{
    ViewData["Title"] = "DPLK | Perta Life";
    Layout = "~/Views/Shared/DPLK_Layout.cshtml";
}

<div class="container">
    <h2>Create New Citizenship</h2>
    <form asp-action="ParameterMakerCitizenshipCreate" method="post">
        <div class="form-group">
            <label for="CitizenshipCd"> ID:</label>
            <input type="text" class="form-control" id="CitizenshipCd" name="CitizenshipCd" required>
        </div>
        <div class="form-group">
            <label for="CitizenshipNm">Desciption:</label>
            <input type="text" class="form-control" id="CitizenshipNm" name="CitizenshipNm" required>
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
        <a asp-action="ParameterMakerCitizenship" class="btn btn-secondary">Back to List</a>
    </form>
</div>
