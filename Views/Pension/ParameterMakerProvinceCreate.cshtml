@model DPLK.Models.Province

@{
    ViewData["Title"] = "DPLK | Perta Life";
    Layout = "~/Views/Shared/DPLK_Layout.cshtml";
}

<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Pension /</span> Create New Province</h4>

    <form asp-action="ParameterMakerProvinceCreate" method="post">
        <div class="form-group">
            <label for="ProvinceNmbr">ID:</label>
            <input type="number" class="form-control" id="ProvinceNmbr" name="ProvinceNmbr" required>
        </div>
        <div class="form-group">
            <label for="ProvinceNm">Description:</label>
            <input type="text" class="form-control" id="ProvinceNm" name="ProvinceNm" required>
        </div>
        <div class="form-group">
            <label for="OldTypeNmbr">Old Type Number:</label>
            <input type="text" class="form-control" id="OldTypeNmbr" name="OldTypeNmbr">
        </div>
        <div class="form-group">
            <label for="LastChangeDt">Last Change Date:</label>
            <input type="datetime-local" class="form-control" id="LastChangeDt" name="LastChangeDt">
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
        <a asp-action="ParameterMakerProvince" class="btn btn-secondary">Back to List</a>
    </form>
</div>
